<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" actionBarHidden="true" navigatedTo="navigatedToPage">
  <StackLayout>
    <Label text="Leaderboard" class="title-leaderboard"/>
    <ListView items="{{highscores}}">
       <ListView.itemTemplate>
           <Label class="course-title" text="{{$value}}" />
       </ListView.itemTemplate>
    </ListView>
  </StackLayout>
</Page>
